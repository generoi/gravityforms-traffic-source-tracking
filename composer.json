{
  "name": "generoi/gravityforms-traffic-source-tracking",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "GravityForms integration for tracking visitors´ initial traffic source",
  "homepage": "https://github.com/generoi/gravityforms-traffic-source-tracking",
  "authors": [
    {
      "name": "Susanna Häggblom",
      "email": "susanna.haggblom@genero.fi"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/generoi/gravityforms-traffic-source-tracking/issues"
  },
  "repositories": [
    { "type": "composer", "url": "https://wpackagist.org" }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  },
  "archive": {
    "exclude": [
      ".gitignore"
    ]
  }
}
